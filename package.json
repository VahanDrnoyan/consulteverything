{
  "name": "consulteverything.com",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "codegen": "graphql-codegen --config codegen.yml",
    "generate:nexus": "nodemon --exec 'ts-node --transpile-only -P nexus.tsconfig.json pages/api/graphql' --ext 'ts' --watch '*/graphql/**/*.ts'",
    "generate": "concurrently \"npm run generate:nexus\" \"npm run codegen\""
  },
  "dependencies": {
    "@apollo/client": "^3.7.0",
    "@genql/runtime": "^2.10.0",
    "@graphql-yoga/node": "^2.13.13",
    "@next-auth/prisma-adapter": "^1.0.4",
    "@paljs/cli": "^4.1.0",
    "@paljs/nexus": "^4.1.0",
    "@paljs/plugins": "^4.1.0",
    "@prisma/client": "^4.4.0",
    "apollo-server-core": "^3.10.2",
    "codegen": "^0.1.0",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.18.0",
    "next": "12.3.1",
    "next-auth": "^4.12.2",
    "nexus": "^1.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@genql/cli": "^2.10.0",
    "@graphql-codegen/cli": "^2.13.3",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@graphql-codegen/typescript-react-apollo": "^3.3.3",
    "@types/node": "^15.12.2",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "concurrently": "^7.4.0",
    "eslint": "8.22.0",
    "eslint-config-next": "12.3.1",
    "nodemon": "^2.0.20",
    "prisma": "^4.4.0",
    "ts-node": "^10.9.1",
    "typescript": "4.8.4"
  }
}
